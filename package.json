{
  "name": "gestion-fichas",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200 -o --configuration=local",
    "start:development": "ng serve --port 4200 -o --configuration=development",
    "start:testing": "ng serve --port 4200 -o --configuration=testing",
    "start:production": "ng serve --port 4200 -o --configuration=production",
    "build": "ng build",
    "build:development": "ng build --configuration=development --base-href /GestionFichas/",
    "build:testing": "ng build --configuration=testing --base-href /GestionFichas-Test/",
    "build:production": "ng build --configuration=production --base-href /GestionFichas/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "ngx-cookie-service": "^15.0.0",
    "rxjs": "~7.8.0",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primeng": "^16.9.1",
    "remixicon": "^4.2.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.14",
    "@angular/cli": "^16.2.14",
    "@angular/compiler-cli": "^16.2.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.1.3"
  }
}
